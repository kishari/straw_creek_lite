package MTPLTariffRules

#list any import classes here.
import hu.dbx.screek.model.facts.*;
import hu.dbx.screek.model.*;

rule "validate postcode"
    agenda-group "valid-tariff"
	when
		$p : Partner( $pCode : address.postCode )
		not(exists AreaDef( postCode == $pCode ) )
		
	then
		insert( Message.create("1") );
		
end